<?xml version="1.0"?>
<launch>
    <arg name="name" default="hotdec_car"/>
     <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find cym_gazebo)/worlds/iccps2020.world"/>
        <!-- more default parameters can be changed here -->
    </include>
    <include file="$(find cym_device)/launch/cym_car.launch">
      <arg name="name" value="$(arg name)"/>
      <arg name="pos_tracker" value="$(arg name)"/>
      <arg name="car_model" value="boxcar"/>
      <arg name= "x" default="-5.0" />
      <arg name= "y" default="5.0" />
    </include>
    <node name="vrpn_client_node" pkg="cym_device" type="cym_vrpn.py" args="$(arg name)"/>
</launch>
